---
import Bento from '../Bento.astro'
import PreFooter from '../PreFooter.astro'
import Footer from '../Footer.astro'
import Intro from '../Intro.astro'
import Video from '../Video.astro'
import Layout from '../../layouts/Layout.astro'

import { getI18n } from '../../i18n'
const i18N = getI18n({ currentLocale: Astro?.currentLocale ?? 'es'})
---

<Layout
  title={i18N.SEO_TITLE}
  description={i18N.DESCRIPTION_TITLE}
  >
  <Video />
  <main>
    <Intro />
    <Bento />
    <PreFooter />
  </main>
  
  <Footer />
</Layout>

<style>

</style>
